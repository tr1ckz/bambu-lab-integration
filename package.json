{
  "name": "bambu-lab-integration",
  "version": "1.0.0",
  "description": "Web application for Bambu Lab printer integration - pulls models and timelapses",
  "main": "simple-server.js",
  "scripts": {
    "start": "node simple-server.js",
    "dev": "nodemon simple-server.js",
    "dev:frontend": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "docker:build": "docker build -t bambu-lab-integration:latest .",
    "docker:run": "docker run -d --name bambu-lab -p 3000:3000 -v bambu_data:/app/data -v bambu_library:/app/library -v bambu_sessions:/app/sessions bambu-lab-integration:latest"
  },
  "keywords": [
    "bambu",
    "3d-printer",
    "timelapse",
    "bambu-lab"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@napi-rs/canvas": "^0.1.83",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/three": "^0.181.0",
    "@vitejs/plugin-react": "^5.1.1",
    "adm-zip": "^0.5.16",
    "axios": "^1.6.0",
    "basic-ftp": "^5.0.5",
    "better-sqlite3": "^12.5.0",
    "canvas": "^3.2.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "fluent-ffmpeg": "^2.1.3",
    "ftp": "^0.3.10",
    "gl": "^8.1.6",
    "http-proxy-middleware": "^3.0.5",
    "jszip": "^3.10.1",
    "mqtt": "^5.14.1",
    "multer": "^2.0.2",
    "openid-client": "^6.8.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-openidconnect": "^0.1.2",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "session-file-store": "^1.5.0",
    "three": "^0.181.2",
    "threejs-webworker-3mf-loader": "github:manyfold3d/threejs-webworker-3mf-loader",
    "typescript": "^5.9.3",
    "vite": "^7.2.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
